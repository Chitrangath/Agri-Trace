#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/bin/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/jshint@2.13.6/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/bin/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/jshint@2.13.6/node_modules/jshint/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/jshint@2.13.6/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jshint/bin/jshint" "$@"
else
  exec node  "$basedir/../jshint/bin/jshint" "$@"
fi
