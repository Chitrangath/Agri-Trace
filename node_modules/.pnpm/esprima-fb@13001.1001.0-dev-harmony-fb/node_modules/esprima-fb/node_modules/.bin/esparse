#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/esprima-fb@13001.1001.0-dev-harmony-fb/node_modules/esprima-fb/bin/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/esprima-fb@13001.1001.0-dev-harmony-fb/node_modules/esprima-fb/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/esprima-fb@13001.1001.0-dev-harmony-fb/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/esprima-fb@13001.1001.0-dev-harmony-fb/node_modules/esprima-fb/bin/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/esprima-fb@13001.1001.0-dev-harmony-fb/node_modules/esprima-fb/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/esprima-fb@13001.1001.0-dev-harmony-fb/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esparse.js" "$@"
else
  exec node  "$basedir/../../bin/esparse.js" "$@"
fi
