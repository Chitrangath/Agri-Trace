#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/forge@2.3.0/node_modules/forge/bin/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/forge@2.3.0/node_modules/forge/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/forge@2.3.0/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/forge@2.3.0/node_modules/forge/bin/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/forge@2.3.0/node_modules/forge/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/forge@2.3.0/node_modules:/home/heisenberg_gg/Agri-Supply-Chain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/forge@2.3.0/node_modules/forge/bin/forge" "$@"
else
  exec node  "$basedir/../.pnpm/forge@2.3.0/node_modules/forge/bin/forge" "$@"
fi
